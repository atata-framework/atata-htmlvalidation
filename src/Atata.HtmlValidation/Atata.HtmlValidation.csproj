<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <TargetFramework>netstandard2.0</TargetFramework>
    <Version>1.0.0</Version>
    <Description>.NET library that adds HTML page validation to Atata using html-validate NPM package.

      Targets .NET Standard 2.0.</Description>
    <RepositoryUrl>https://github.com/atata-framework/atata-htmlvalidation</RepositoryUrl>
    <PackageId>Atata.HtmlValidation</PackageId>
    <PackageTags>atata automation testing test selenium webdriver html</PackageTags>
    <PackageReleaseNotes>Initial release.</PackageReleaseNotes>
    <PackageProjectUrl>https://github.com/atata-framework/atata-htmlvalidation</PackageProjectUrl>
  </PropertyGroup>

  <ItemGroup>
    <PackageReference Include="Atata" Version="1.11.2" />
    <PackageReference Include="Atata.Cli.HtmlValidate" Version="1.0.0" />
    <PackageReference Include="Atata.Cli.Npm" Version="1.0.0" />
  </ItemGroup>

</Project>
